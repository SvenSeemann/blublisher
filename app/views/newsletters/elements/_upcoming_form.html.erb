<%= form_tag '/newsletter/element/save', remote: true, onsubmit: 'set_parent_to_modify(this)' do %>
  <%= hidden_field_tag :element_type, 'Upcoming' %>
  <%= hidden_field_tag :newsletter_id, Newsletter.last.id %>
  Von: <%= date_select("upcoming", "from", start_year: 2015) %>
  <br>
  Bis: <%= date_select("upcoming", "to", start_year: 2015) %>
  <br>
  <%= submit_tag %>
<% end %>

